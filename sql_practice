MariaDB [mydata]>  select title,author_lname,
    -> case
    ->  when title like '%stories%' then 'short stories'
    ->  when title like '%just kids%' then 'Memoir'
    ->  when title like '%A heartbreaking work%' then 'Memoir'
    -> else 'Novel'
    ->  end as TYPE
    ->  from books;
